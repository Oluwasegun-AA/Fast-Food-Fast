  "license": "MIT",
  "dependencies": {
    "body-parser": "1.18.3",
    "dotenv": "6.0.0",
    "express": "4.16.3",
    "express-validator": "5.3.0",
    "morgan": "1.9.0",
    "pg": "7.4.3"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
